<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ContatosBundle\Entity\Contatos" table="contatos">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nome" type="string" column="nome" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tell" type="string" column="tell" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cell" type="string" column="cell" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="bairro" type="string" column="bairro" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="status" type="string" column="status" length="1" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
